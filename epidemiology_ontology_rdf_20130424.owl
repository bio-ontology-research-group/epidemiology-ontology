    <!-- http://epidemiology_ontology.owl#EO:0000117 -->

    <Class rdf:about="&epidemiology_ontology;EO:0000117">
        <rdfs:label>mantel-haenszel estimate, mantel-haenszel odds ratio</rdfs:label>
        <rdfs:subClassOf rdf:resource="&epidemiology_ontology;EO:0000102"/>
